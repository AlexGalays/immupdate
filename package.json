{
  "name": "immupdate",
  "version": "1.1.3",
  "description": "Immutable update for Objects and Arrays",
  "license": "MIT",

  "main": "commonjs/immupdate",
  "module": "es/immupdate",
  "typings": "commonjs/immupdate.d.ts",

  "devDependencies": {
    "typescript": "2.4.1",
    "mocha": "2.2.5",
    "expect": "1.8.0",
    "chalk": "1.1.1"
  },

  "scripts": {
    "build": "npm run build-commonjs && npm run build-es",
    "build-es": "tsc immupdate.ts --outDir es --strict --noUnusedParameters --declaration --lib es5,es6 --module es6",
    "build-commonjs": "tsc immupdate.ts --outDir commonjs --strict --noUnusedParameters --declaration --lib es5,es6",
    "pretest": "npm run build && tsc test/mocha.d.ts test/global.d.ts test/test.ts --strict",
    "test": "mocha test/test.js && node test/testCompilationErrors.js"
  },

  "files": [
    "lib"
  ],

  "repository": {
    "type": "git",
    "url": "git://github.com/AlexGalays/immupdate.git"
  },
  "keywords": [
    "immutable",
    "update",
    "clone",
    "collection",
    "Array",
    "Object",
    "model",
    "data",
    "typescript"
  ],
  "author": "AlexGalays",
  "homepage": "https://github.com/AlexGalays/immupdate"
}
